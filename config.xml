<?xml version='1.0' encoding='utf-8'?>
<widget id="org.marshallfredericks" version="1.0.8">
    <name>MFSM</name>
    <description>Marshall M. Fredericks Sculpture Museum</description>
    <author>Adam Pero, Ka Vang</author>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="UIWebViewBounce" value="false" />
    <preference name="android-minSdkVersion" value="14"/>
    <preference name="orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.3.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <platform name="ios">
        <icon height="29" platform="ios" src="www/res/icon/ios/29.png" width="29" />
        <icon height="40" platform="ios" src="www/res/icon/ios/40.png" width="40" />
        <icon height="50" platform="ios" src="www/res/icon/ios/50.png" width="50" />
        <icon height="57" platform="ios" src="www/res/icon/ios/57.png" width="57" />
        <icon height="58" platform="ios" src="www/res/icon/ios/58.png" width="58" />
        <icon height="60" platform="ios" src="www/res/icon/ios/60.png" width="60" />
        <icon height="72" platform="ios" src="www/res/icon/ios/72.png" width="72" />
        <icon height="76" platform="ios" src="www/res/icon/ios/76.png" width="76" />
        <icon height="80" platform="ios" src="www/res/icon/ios/80.png" width="80" />
        <icon height="87" platform="ios" src="www/res/icon/ios/87.png" width="87" />
        <icon height="100" platform="ios" src="www/res/icon/ios/100.png" width="100" />
        <icon height="114" platform="ios" src="www/res/icon/ios/114.png" width="114" />
        <icon height="120" platform="ios" src="www/res/icon/ios/120.png" width="120" />
        <icon height="144" platform="ios" src="www/res/icon/ios/144.png" width="144" />
        <icon height="152" platform="ios" src="www/res/icon/ios/152.png" width="152" />
        <icon height="180" platform="ios" src="www/res/icon/ios/180.png" width="180" />
